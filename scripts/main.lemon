//This is an example of a multi-char mod
//I put all the stuff into one folder for some reason
include charscripts/?

global u8 surge.palette
constant u8 CHARACTER_SURGE = 4

function string getCharacterPaletteKey(u8 character)
{
	if (character == CHARACTER_SURGE)
	{
		return "surge_" + surge.palette
	}

	return base.getCharacterPaletteKey(character)
}

	